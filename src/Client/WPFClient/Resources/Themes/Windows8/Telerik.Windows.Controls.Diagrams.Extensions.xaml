                            <ListBox
                                    Grid.Row="3"
                                    ItemsSource="{data:FontColors}"
                                    Style="{StaticResource PaneListBoxStyle}"
                                    Margin="0 10 0 0"
                                    extensions:SettingsPaneView.EditorPropertyName="Foreground"
                                    extensions:SettingsPaneView.EditorItemType="Shapes, Connections"
                                    extensions:SettingsPaneView.EditorValue="{Binding Path=SelectedItem.Fill, Mode=TwoWay, RelativeSource={RelativeSource Self}}"/>
