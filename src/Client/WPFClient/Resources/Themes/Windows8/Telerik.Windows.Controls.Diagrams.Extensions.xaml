                                        FontSize="{StaticResource ZoomTextBoxFontSize}"
                                        HorizontalAlignment="Right"
                                        Grid.Column="3"
                                        VerticalAlignment="Center"
                                        Width="{StaticResource ZoomTextBoxWidth}"
