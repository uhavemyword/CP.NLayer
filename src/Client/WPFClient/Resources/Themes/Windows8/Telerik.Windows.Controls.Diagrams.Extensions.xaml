    <Style TargetType="extensions:RadDiagramThumbnail" x:Key="RadDiagramThumbnailStyle">
        <Setter Property="Background" Value="{StaticResource DiagramThumbNailBackground}"/>
        <Setter Property="BorderBrush" Value="{StaticResource DiagramThumbNailBorderBrush}"/>
        <Setter Property="BorderThickness" Value="{StaticResource DiagramThumbNailThickness}"/>
        <Setter Property="Height" Value="280"/>
        <Setter Property="Width" Value="280"/>
        <Setter Property="ViewportStyle">
            <Setter.Value>
                <Style TargetType="Rectangle">
