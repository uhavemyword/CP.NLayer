    <Style TargetType="extensions:SettingsPaneTextControl" x:Key="SettingsPaneTextControlStyle">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="extensions:SettingsPaneTextControl">
