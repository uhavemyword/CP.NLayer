                        <extensions:DiagramScaleItemDefinition Interval="10" Type="Label"/>
                        <extensions:DiagramScaleItemDefinition Interval="50" Type="Label"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="Label"/>
                    </extensions:DiagramScaleDefinition>
                </extensions:DiagramScaleDefinitionCollection>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:XSmallTickContainer" BasedOn="{StaticResource XSmallTickContainerStyle}"/>
    <Style TargetType="extensions:SmallTickContainer" BasedOn="{StaticResource SmallTickContainerStyle}"/>
    <Style TargetType="extensions:MediumTickContainer" BasedOn="{StaticResource MediumTickContainerStyle}"/>
    <Style TargetType="extensions:LargeTickContainer" BasedOn="{StaticResource LargeTickContainerStyle}"/>
    <Style TargetType="extensions:LabelContainer" BasedOn="{StaticResource LabelContainerStyle}"/>
    <Style TargetType="extensions:RadDiagramRuler" BasedOn="{StaticResource RadDiagramRulerStyle}"/>
    <extensions:PercentageConverter x:Key="DoublePercentageConverter"/>
    <extensions:ReciprocalDoubleConverter x:Key="ReciprocalDoubleConverter"/>
    <ControlTemplate x:Key="PrintPreviewNonEditableComboBox" TargetType="telerik:RadComboBox">
        <Grid x:Name="VisualRoot">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Disabled"/>
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard/>
                    </VisualState>
                    <VisualState x:Name="DropDownOpen">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="FocusBorder" Storyboard.TargetProperty="Opacity" To="0"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="FocusBorder">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Unfocused"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="BackgroundBorder"
                    IsHitTestVisible="False"
                    Background="{TemplateBinding Background}"
                    Grid.ColumnSpan="2"
                    BorderThickness="1"
                    BorderBrush="{TemplateBinding BorderBrush}"/>
            <ContentControl
                    Margin="12 0 0 0"
                    Content="{TemplateBinding SelectionBoxItem}"
                    VerticalAlignment="Center"
                    HorizontalContentAlignment="Left"
                    VerticalContentAlignment="Center"
                    HorizontalAlignment="Stretch"
                    ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                    Foreground="{TemplateBinding Foreground}"/>
            <telerik:RadToggleButton x:Name="PART_DropDownButton" Grid.Column="1" IsTabStop="False" ClickMode="Press" Padding="1 0 0 0" Margin="0">
                <telerik:RadToggleButton.ContentTemplate>
                    <DataTemplate>
                        <ContentControl x:Name="DropDownIcon"
                                IsTabStop="False"
                                Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType=ContentControl}}"
                                Background="{StaticResource ButtonIconBackground_Normal}"
                                Template="{StaticResource PrintArrowTemplateCombobox}"/>
                    </DataTemplate>
                </telerik:RadToggleButton.ContentTemplate>
            </telerik:RadToggleButton>
            <Border x:Name="FocusBorder"
                    IsHitTestVisible="False"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Visibility="Collapsed"
                    Background="Transparent"
                    Grid.ColumnSpan="2"
                    Opacity="0.2"
                    BorderBrush="{TemplateBinding BorderBrush}"/>
            <Popup x:Name="PART_Popup" VerticalOffset="1" Margin="0 1" AllowsTransparency="True">
                <Grid x:Name="PopupRoot">
                    <Border
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            MinWidth="{TemplateBinding MinDropDownWidth}"
                            MaxHeight="{TemplateBinding MaxDropDownHeight}"
                            Background="{StaticResource ComboPopupBackground}"
                            CornerRadius="{StaticResource SplitButton_SpanCornerRadius}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <telerik:RadButton x:Name="PART_ClearButton"
                                    Grid.Row="0"
                                    Margin="-1 -1 -1 0"
                                    Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
                                    Content="{TemplateBinding ClearSelectionButtonContent}"/>
                            <ScrollViewer x:Name="PART_ScrollViewer"
                                    Grid.Row="1"
                                    Padding="0 1 0 0"
                                    BorderThickness="0"
                                    VerticalScrollBarVisibility="Auto"
                                    Foreground="{TemplateBinding Foreground}"
                                    CanContentScroll="True">
                                <ItemsPresenter/>
                            </ScrollViewer>
                        </Grid>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
    </ControlTemplate>
    <DataTemplate x:Key="PageSizesSelectedItemTemplate">
        <StackPanel Orientation="Horizontal">
            <Path
                    Stretch="Fill"
                    Stroke="{Binding Foreground, ElementName=PageMediaSizeName}"
                    Width="8"
                    Height="11"
                    Data="F1M222,210L222,266.711C222,269.545,222.775,271,225.5,271L272.119,271C274.76,271,276,270.603,276,266.711L276,234.32C276,232.594,275.557,229.807,273.375,227.625C270.219,224.469,261.051,215.65,259.015,213.614C256.979,211.578,255.875,210,249.503,210z"/>
            <TextBlock x:Name="PageMediaSizeName" Text="{Binding PageMediaSizeName}" Margin="5 0 0 0"/>
        </StackPanel>
    </DataTemplate>
    <Style x:Key="PrintPreviewComboStyle" TargetType="telerik:RadComboBox">
        <Setter Property="ItemContainerStyle" Value="{StaticResource RadComboBoxItemStyle}"/>
        <Setter Property="Template" Value="{StaticResource PrintPreviewNonEditableComboBox}"/>
        <Setter Property="NonEditableTemplate" Value="{StaticResource PrintPreviewNonEditableComboBox}"/>
        <Setter Property="BorderBrush" Value="{StaticResource PrintPreviewBorderBrush}"/>
        <Setter Property="Background" Value="{StaticResource PrintPreviewComboBackground}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 0 0 10"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreviewItem" x:Key="RadDiagramPrintPreviewItemStyle">
        <Setter Property="Margin" Value="2"/>
        <Setter Property="BorderBrush" Value="{StaticResource PrintPreviewBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:RadDiagramPrintPreviewItem">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <Border.Effect>
                            <DropShadowEffect ShadowDepth="1" BlurRadius="4" Color="#FD000000" Opacity="0.3"/>
                        </Border.Effect>
                        <Border BorderBrush="#33000000" Background="Transparent" BorderThickness="{TemplateBinding BorderThickness}" Margin="{TemplateBinding Padding}">
                            <Border BorderBrush="#66FFFFFF" Margin="1" Background="Transparent" BorderThickness="{TemplateBinding BorderThickness}">
                                <Image
                                        Stretch="Uniform"
                                        Source="{TemplateBinding PagePreviewImageSource}"
                                        Width="{TemplateBinding PagePreviewImageWidth}"
                                        Height="{TemplateBinding PagePreviewImageHeight}"/>
                            </Border>
                        </Border>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreview" x:Key="RadDiagramPrintPreviewStyle">
        <Setter Property="MinWidth" Value="200"/>
        <Setter Property="MinHeight" Value="200"/>
        <Setter Property="Background" Value="{StaticResource PrintPreviewBackground}"/>
        <Setter Property="BorderBrush" Value="{StaticResource PrintPreviewBorderBrush}"/>
        <Setter Property="ItemStyle" Value="{StaticResource RadDiagramPrintPreviewItemStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Background="{TemplateBinding Background}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition MaxWidth="280"/>
                        </Grid.ColumnDefinitions>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" Background="{StaticResource PrintPreviewPagesBackground}" BorderThickness="0">
                                <telerik:RadUniformGrid x:Name="PART_PagesPanel" VerticalAlignment="Top" UseLayoutRounding="True" HorizontalAlignment="Left" Margin="15"/>
                            </ScrollViewer>
                            <StackPanel Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center" Orientation="Horizontal" Margin="{StaticResource PrintPreviewZoomePanePadding}">
                                <TextBlock
                                        VerticalAlignment="Center"
                                        Text="{telerik:LocalizableResource Key=Zoom}"
                                        FontWeight="{StaticResource PrintPreviewTextBlockFontWeight}"
                                        FontFamily="{StaticResource PrintPreviewTextBlockFontFamily}"
                                        FontSize="{StaticResource PrintPreviewZoomPaneTextBlockFontSize}"
                                        Foreground="{StaticResource PrintPreviewTextBlockForeground}"/>
                                <TextBlock
                                        VerticalAlignment="Center"
                                        Margin="0,0,4,0"
                                        FontWeight="{StaticResource PrintPreviewTextBlockFontWeight}"
                                        FontFamily="{StaticResource PrintPreviewTextBlockFontFamily}"
                                        FontSize="{StaticResource PrintPreviewZoomPaneTextBlockFontSize}"
                                        Text="{Binding Value, ElementName=ZoomSlider, Converter={StaticResource DoublePercentageConverter}}"
                                        Foreground="{StaticResource PrintPreviewTextBlockForeground}"/>
                                <telerik:RadSlider x:Name="ZoomSlider"
                                        TickFrequency="1"
                                        MinWidth="124"
                                        VerticalAlignment="Center"
                                        Minimum="0.1"
                                        Maximum="5"
                                        SmallChange="0.01"
                                        LargeChange="0.1"
                                        Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ScaleDownFactor, Mode=TwoWay, Converter={StaticResource ReciprocalDoubleConverter}}"/>
                            </StackPanel>
                            <Rectangle Grid.RowSpan="2" Width="1" Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Right" VerticalAlignment="Stretch"/>
                            <Rectangle Height="1" Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Bottom"/>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock
                                    Margin="15,15,15,0"
                                    HorizontalAlignment="Left"
                                    FontFamily="{StaticResource PrintPreviewTextBlockFontFamily}"
                                    FontWeight="{StaticResource PrintPreviewTextBlockFontWeight}"
                                    FontSize="{StaticResource PrintPreviewTextBlockFontSize}"
                                    Foreground="{StaticResource PrintPreviewTextBlockForeground}"
                                    Text="{telerik:LocalizableResource Key=PrintPreview_Print}"/>
                            <telerik:RadComboBox
                                    Grid.Row="1"
                                    Margin="15,10,15,0"
                                    ItemsSource="{Binding PrintQueues, Mode=OneWay}"
                                    SelectedItem="{Binding SelectedPrintQueue, Mode=TwoWay}"
                                    DisplayMemberPath="Name"
                                    Style="{StaticResource PrintPreviewComboStyle}"/>
                            <TextBlock
                                    Grid.Row="2"
                                    Margin="15,15,15,0"
                                    HorizontalAlignment="Left"
                                    FontFamily="{StaticResource PrintPreviewTextBlockFontFamily}"
                                    FontWeight="{StaticResource PrintPreviewTextBlockFontWeight}"
                                    FontSize="{StaticResource PrintPreviewTextBlockFontSize}"
                                    Foreground="{StaticResource PrintPreviewTextBlockForeground}"
                                    Text="{telerik:LocalizableResource Key=PrintPreview_Settings}"/>
                            <telerik:RadComboBox
                                    Grid.Row="3"
                                    Margin="15,10,15,0"
                                    ItemsSource="{Binding PageOrientations, Mode=OneWay}"
                                    SelectedItem="{Binding SelectedPageOrientation, Mode=TwoWay}"
                                    DisplayMemberPath=""
                                    Style="{StaticResource PrintPreviewComboStyle}"/>
                            <telerik:RadComboBox
                                    Grid.Row="4"
                                    Margin="15,15,15,0"
                                    ItemsSource="{Binding PageSizes, Mode=OneWay}"
                                    SelectedItem="{Binding SelectedPageSize, Mode=TwoWay}"
                                    DisplayMemberPath="PageMediaSizeName"
                                    SelectionBoxTemplate="{StaticResource PageSizesSelectedItemTemplate}"
                                    Style="{StaticResource PrintPreviewComboStyle}"/>
                            <telerik:RadButton x:Name="PART_PrintButton"
                                    Grid.Row="6"
                                    HorizontalContentAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    Margin="15 14"
                                    Content="{telerik:LocalizableResource Key=PrintPreview_Print}"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreview" BasedOn="{StaticResource RadDiagramPrintPreviewStyle}"/>
    <Style TargetType="extensions:RadDiagramPrintPreviewItem" BasedOn="{StaticResource RadDiagramPrintPreviewItemStyle}"/>
</ResourceDictionary>
